</video>-->

<script type="text/javascript">
	var username = getCookie("username");
	socket.emit('type', {
		type : 'admin',
		url : 'create',
		username : username
	});
	var roomId;

	socket.on('store', function(data){

		roomId = data.id;
		pos = data.pos;
		console.log("positions are "+pos);
		document.querySelector("a-camera").setAttribute("position", {x:pos[0] ,y: pos[1]+1.5 ,z: pos[2]});
	});
</script>
